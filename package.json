{
  "name": "e-shop",
  "description": "A Vue.js project",
  "version": "1.0.0",
  "author": "Luo Reid <luo.reid@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev-bark": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "dev": "node server",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "db": "json-server --watch db.json",
    "eslint": "eslint --ext .js,.jsx,.vue src --fix",
    "jest": "jest",
    "jest:update": "jest --updateSnapshot",
    "build:client": "cross-env NODE_ENV=production webpack --progress --hide-modules --config webpack/client.js",
    "build:server": "cross-env NODE_ENV=production webpack --progress --hide-modules --config webpack/server.js",
    "build1": "rimraf dist && npm run build:client && npm run build:server",
    "start": "cross-env NODE_ENV=production node server"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.16.4",
    "reify": "^0.18.1",
    "vue": "^2.5.11",
    "vue-router": "^3.0.2",
    "vuex": "^3.1.0",
    "vuex-router-sync": "^5.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "autoprefixer": "^9.5.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "chokidar": "^2.1.5",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.4",
